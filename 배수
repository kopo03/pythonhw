# n의 배수 구하기
import random
firsts = [[5, 7, 12, 20, 25, 26], # 1번
          [30, 31, 34, 39, 41, 45], # 2번
          [10, 16, 19, 27, 37, 38], # 3번
          [4, 8, 22, 26, 32, 38], # 4번
          [1, 9, 10, 13, 35, 44]] # 5번
cnt_2 = []
cnt_3 = []
cnt_5 = []
cnt_7 = []

for i in range(1, 46):
  if i in firsts[0]:
    if i % 2 == 0:
      cnt_2.append(i)
    if i % 3 == 0:
      cnt_3.append(i)
    if i % 5 == 0:
      cnt_5.append(i)
    if i % 7 == 0:
      cnt_7.append(i)
print('1번 로또의 2의 배수 :',cnt_2, '3의 배수 :', cnt_3, '5의 배수 :', cnt_5, '7의 배수 :', cnt_7)

cnt_2 = []
